<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.engin.testgame">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".savedGames.SavedGamesActivity"></activity>
        <activity android:name=".archive.AddArchiveActivity" />
        <activity android:name=".archive.ArchiveActivity" />
        <activity android:name=".archive.ArchiveDetailActivity" />
        <activity android:name=".archive.ArchiveListAdapter" />
        <activity android:name=".archive.ArchiveListActivity" />
        <activity android:name=".HomePageActivity" />
        <activity android:name=".SignInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DetailActivity" />
        <activity android:name=".LeaderBoardActivity" />
        <activity android:name=".event.EventDetailActivity" />
        <activity android:name=".event.EventListActivity" />
        <activity android:name=".achievement.AchievementDetailActivity" />
        <activity android:name=".achievement.AchievementListActivity" />
        <activity android:name=".achievement.AchievementActivity" />
        <activity android:name=".GameActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>